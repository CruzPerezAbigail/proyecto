<%= stylesheet_link_tag "style" %>
</head>

<div class="container">
<div class="card mt-5">
<div class="card-header">
<ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="">Material</a></li>
          <li class="breadcrumb-item active">Caracteristicas material</li>
        </ol>
                  <h4 class="text">Material  <%= @physical.id%> </h4>
</div>

<div class="probar">
<%= render @physical %>

<div>
<% if can? :edit, Physical%>
  <%= link_to "Editar material", edit_physical_path(@physical), class: "btn btn-success btn-xs round" %>  <%end%> |
  <%= link_to "Lista", physicals_path, class: "btn btn-success btn-xs round" %>
   <% if can? :destroy, Physical%>
  <%= button_to "Eliminar material", @physical, method: :delete, data: { confirm: "Esta seguro de eliminar"},  class: "btn btn-danger" %>
  <%end%>
</div>
