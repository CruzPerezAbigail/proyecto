<head>
<%= stylesheet_link_tag "style" %>
</head>

<div class="container">
<div class="card mt-5">
<div class="card-header">
<ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="">Reporte</a></li>
          <li class="breadcrumb-item active">Caracteristicas reporte</li>
        </ol>
                  <h4 class="text">Reporte  <%= @reporte.id%> </h4>

</div>
 <div class="probar">
<%= render @reporte %>

<div>
<% if can? :edit, Reporte%>
  <%= link_to "Editar reporte", edit_reporte_path(@reporte),  class: "btn btn-success" %> <%end%> |
  <%= link_to "Regresar reportes", reportes_path,  class: "btn btn-success" %>

<%if can? :destroy, Reporte%>
  <%= link_to "Eliminar reporte", @reporte, method: :delete, data: { confirm: "Esta seguro de eliminar"}, class: "btn btn-danger" %>
<%end%>
</div>
