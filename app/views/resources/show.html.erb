<head>
<%= stylesheet_link_tag "style" %>
</head>
<ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="">Insumo</a></li>
          <li class="breadcrumb-item active">Caracteristicas Insumo</li>
        </ol>
<div class="container">
<div class="card mt-5">
<div class="card-header">


<h4 class="text">Caracteristicas insumo <%= @resource.id %> </h4>
</div>
 <div class="probar">

<p style="color: green"><%= notice %></p>

<%= render @resource %>

<div>
<%if can? :edit, Resource%>
  <%= link_to "Editar insumo", edit_resource_path(@resource), class: "btn btn-success btn-xs round" %> <%end%> |
  <%= link_to "Regresar insumos", resources_path, class: "btn btn-success btn-xs round" %>
<%if can? :destroy, Resource%>
  <%= button_to "Eliminar insumo", @resource, method: :delete , data: { confirm: "Esta seguro de eliminar"},  class: "btn btn-danger"%>
  <%end%>
</div>
