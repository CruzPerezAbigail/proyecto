<head>
<%= stylesheet_link_tag "style" %>
</head>
<div class="container">
<div class="card mt-5">
<div class="card-header">
<ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="">Tipo</a></li>
          <li class="breadcrumb-item active"> Caracteristicas de Tipo</li>
        </ol>


<h4 class="text"> Tipo con identificador <%= @shape.id %> </h4>
</div>
 <div class="probar">



<%= render @shape %>

<div>
<%if can? :edit, Shape%>
  <%= link_to "Editar  tipo", edit_shape_path(@shape),  class: "btn btn-success" %> <%end%> |
  <%= link_to "Lista de tipos", shapes_path ,  class: "btn btn-success"%> |
<%if can? :destroy, Shape%>
  <%= button_to "Eliminar tipo", @shape, method: :delete , data: { confirm: "Esta seguro de eliminar"},  class: "btn btn-danger" %>
  <%end%>
</div>
