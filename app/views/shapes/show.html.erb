<head>
<%= stylesheet_link_tag "style" %>
</head>
<ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="">Tipo</a></li>
          <li class="breadcrumb-item active"> Caracteristicas de Tipo</li>
        </ol>
<div class="container">
<div class="card mt-5">
<div class="card-header">

<h4 class="text">Nuevo Tipo <%= @shape.id %> </h4>
</div>
 <div class="probar">

<p style="color: green"><%= notice %></p>

<%= render @shape %>

<div>
  <%= link_to "Editar  tipo", edit_shape_path(@shape),  class: "btn btn-success" %> |
  <%= link_to "Lista de tipos", shapes_path ,  class: "btn btn-success"%> |

  <%= link_to "Eliminar tipo", @shape, method: :delete , data: { confirm: "Esta seguro de eliminar"},  class: "btn btn-danger"%>
</div>
